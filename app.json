{
  "expo": {
    "name": "RecreSystem",
    "slug": "neurecreation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp", 
    "platforms": ["android", "ios", "web"], 
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.NEU.RecreSystem",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "myapp", 
              "host": "auth" 
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "expoClientId": "137089432477-etqjceh3pa6cglba3b20kg0ktj4j25nj.apps.googleusercontent.com",
      "googleClientId": "137089432477-grqu4uo5resnitm26n9vu5mngn9a9so5.apps.googleusercontent.com",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6f074806-2583-42e6-8479-b8d5a97228ab"
      }
    },
    "owner": "aintfritz"
  }
}